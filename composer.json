{
  "name": "opmvpc/advent",
  "description": "advent of code 2020",
  "keywords": [
    "opmvpc",
    "Advent"
  ],
  "homepage": "https://github.com/opmvpc/Advent",
  "license": "MIT",
  "authors": [
    {
      "name": "opmvpc",
      "email": "thibsix@outlook.be",
      "homepage": "https://tsix.be",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpunit/phpunit": "^9.3",
    "symfony/var-dumper": "^5.2",
    "vimeo/psalm": "^3.11"
  },
  "autoload": {
    "psr-4": {
      "Opmvpc\\Advent\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Opmvpc\\Advent\\Tests\\": "tests"
    }
  },
  "scripts": {
    "psalm": "psalm",
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage",
    "format": "php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
